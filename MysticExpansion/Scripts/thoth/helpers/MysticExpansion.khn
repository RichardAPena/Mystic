function SuccessRate(threshold)
    local random_num = math.random(1,100)
    return ConditionResult(random_num <= threshold)
end

function IsSensitiveToSunlight(source, target)
    source = source or context.Source
    target = target or context.Target

    return HasPassive('SunlightSensitivity',target) | HasPassive('SunlightHypersensitivity',target) | HasPassive('SunlightWeakness',target) | HasPassive('SunSickness_Myconid',target)
end

function HeightenedPsionicsCheck()
    return (HasStringInSpellRoll('SavingThrow') | HasStringInFunctorConditions('SavingThrow') | HasStringInSpellRoll('SpellAutoResolveOnAlly') | HasStringInSpellRoll('HeatMetalInitialCheck') | HasStringInSpellRoll('HeatMetalReapplyCheck')) & (HasSpellFlag(SpellFlags.Spell) & (PsionicDisciplineCheck() | PsionicTalentCheck()))
end

function DevastatingPsionicsCheck()
    return HasFunctor(StatsFunctorType.DealDamage) & HasSpellFlag(SpellFlags.Spell) & (PsionicDisciplineCheck() | PsionicTalentCheck())
end

function CharacterLevelLessThan(value, entity)
    entity = entity or context.Source
    return ConditionResult(entity.Level < value, {ConditionError("IsNotCharacterLevelLessThan")})
end

