function GiantGrowthCheck()
    return HasStringInSpellRoll('MeleeUnarmedAttack')
      | HasStringInSpellRoll('MeleeWeaponAttack')
      | HasStringInSpellRoll('MeleeSpellAttack')
end

function SpellPowerLevelEqualOrGreaterThan(value)
    return ConditionResult(context.HitDescription.SpellPowerLevel >= value)
end

function HasMaximumPsionicResonanceCharge(entity)
    local entity = entity or context.Target
    return ConditionResult(GetStatusDuration('MYSTIC_PSIONIC_RESONANCE', entity) > 7)
end

function MysticArsenal_GreatWeapon(entity)
    entity = entity or context.Target
    local meleeMainHand = GetItemInEquipmentSlot(EquipmentSlot.MeleeMainHand, entity)
    local meleeOffHand = GetItemInEquipmentSlot(EquipmentSlot.MeleeOffHand, entity)

    local hasTwohandedWeapon = HasWeaponProperty(WeaponProperties.Twohanded, meleeMainHand) & HasWeaponProperty(WeaponProperties.Melee, meleeMainHand)
    local hasVersatileWeapon = HasWeaponProperty(WeaponProperties.Versatile, meleeMainHand) & ~HasWeaponProperty(WeaponProperties.Melee, meleeOffHand) & ~HasWeaponProperty(WeaponProperties.Ammunition, meleeOffHand) & ~HasShieldEquipped(entity)

    return hasTwohandedWeapon | hasVersatileWeapon
end

function MysticArsenal_Dueling(entity)
    entity = entity or context.Target
    local meleeMainHand = GetItemInEquipmentSlot(EquipmentSlot.MeleeMainHand, entity)
    local meleeOffHand = GetItemInEquipmentSlot(EquipmentSlot.MeleeOffHand, entity)

    local hasNonVersatileWeapon = HasWeaponProperty(WeaponProperties.Melee, meleeMainHand) & ~HasWeaponProperty(WeaponProperties.Versatile, meleeMainHand) & ~HasWeaponProperty(WeaponProperties.Twohanded, meleeMainHand) & ~HasWeaponProperty(WeaponProperties.Melee, meleeOffHand) & ~HasWeaponProperty(WeaponProperties.Ammunition, meleeOffHand)
    local hasVersatileWeapon = HasWeaponProperty(WeaponProperties.Melee, meleeMainHand) & HasWeaponProperty(WeaponProperties.Versatile, meleeMainHand) & HasShieldEquipped(entity)
    
    return hasNonVersatileWeapon | hasVersatileWeapon
end

function TranscendenceCheck()
    return (PsionicDisciplineCheck() | PsionicTalentCheck()) & (SpellTypeIs(SpellType.Target) | SpellTypeIs(SpellType.Projectile)) & ~AreaRadiusGreaterThan(0) & ~HasFunctor(StatsFunctorType.Summon)
end

function SpectralArmsCheck()
   return HasUseCosts('ActionPoint') & HasStringInSpellRoll('WeaponAttack') 
end

